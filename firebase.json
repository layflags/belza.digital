{
  "hosting": {
    "public": "build",
    "cleanUrls": true,
    "trailingSlash": false,

    "headers": [
      {
        "source" : "**",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "public, max-age=31536000, no-cache"
          }
        ]
      }
    ]
  }
}
